{
  "expo": {
    "name": "Codex Phone",
    "slug": "codex-phone",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "codex-phone",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.codex.phone",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0f172a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codex.phone",
      "icon": "./assets/icon.png",
      "appleTeamId": "M4K84L4TKR"
    },
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    }
  }
}
